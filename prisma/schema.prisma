generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id           Int      @id @default(autoincrement())
  features     String[]
  link         String
  company      String
  start_year   Int
  project_name String
}

model Testimonial {
  id          Int     @id @default(autoincrement())
  image       String?
  company     String
  content     String
  designation String
  client_name String
}
